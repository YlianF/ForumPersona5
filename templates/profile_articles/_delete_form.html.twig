<form method="post" action="{{ path('app_profile_articles_delete', {'id': article.id}) }}" onsubmit="return confirm('Vous êtes sûrs de vouloir supprimer cet article ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ article.id) }}">
    <button class="btn">Supprimer</button>
</form>
